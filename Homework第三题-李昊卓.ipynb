   "source": [
    "#输出所有200以内，各个数位之和为偶数的数\n",
    "number=[]\n",
    "final=[]\n",
    "for num in range(2,201):\n",
    "    for i in range(2,num):\n",
    "        if (num%i)==0:\n",
    "            break\n",
    "    else: \n",
    "        number.append(num)\n",
    "        if(num//100 + num//10 + num)%2==0:\n",
    "            final.append(num)\n",
    "final\n"
   ]
  }
